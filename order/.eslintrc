{
  "extends": "ember",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "modules": true
  },
  "env": {
    "browser": true,
    "mocha": true,
    "es6": true,
    "node": true
  },
  "globals": {
    "$": true
  },
  "rules": {
    "array-callback-return": 1,
    "no-process-env": 1,
    "comma-dangle": [2,"never"],
    "no-cond-assign": [2,"always"],
    "no-console": 1,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty-character-class": 2,
    "no-empty": 1,
    "no-ex-assign": 2,
    "no-extra-parens": [1,"all"],
    "no-extra-semi": 2,
    "no-magic-numbers": 0,
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 2,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-typeof": 2,
    "curly": [1,"multi-line"],
    "default-case": 2,
    "dot-notation": 1,
    "eqeqeq": 1,
    "guard-for-in": 1,
    "no-alert": 1,
    "no-else-return": 1,
    "no-fallthrough": 2,
    "no-new": 1,
    "no-octal": 1,
    "no-redeclare": 2,
    "no-delete-var": 2,
    "no-unused-vars": 1,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-mixed-spaces-and-tabs": [1,"smart-tabs"],
    "no-new-object": 1,
    "arrow-parens": 1,
    "arrow-spacing": 1,
    "constructor-super": 1,
    "generator-star-spacing": 1,
    "no-class-assign": 1,
    "no-const-assign": 1,
    "no-this-before-super": 1,
    "no-var": 1,
    "object-shorthand": 0,
    "prefer-const": 1,
    "prefer-spread": 1,
    "prefer-reflect": 1,
    "require-yield": 1,
    "consistent-this": ["error", "self"],
    "prefer-rest-params": 1,
    "consistent-return": 1
  }
}
